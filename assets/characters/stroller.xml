<character depth="2">
	<clips rowsize="16" framewidth="32" frameheight="48" default="down" image="textures/walk_cycle32.png">
		<clip name="up" start="32" count="4" delay="0.1"/>
		<clip name="right" start="36" count="4" delay="0.1"/>
		<clip name="down" start="40" count="4" delay="0.1"/>
